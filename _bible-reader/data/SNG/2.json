        "reference": {
          "chapter": 2,
          "verse": 4
        }
      },
      {
        "noteId": 4755,
        "caller": "+",
        "text": "Or pruning",
        "reference": {
          "chapter": 2,
          "verse": 12
        }
      },
      {
        "noteId": 4756,
        "caller": "+",
        "text": "Or the rugged mountains",
        "reference": {
          "chapter": 2,
          "verse": 17
        }
      }
    ]
  },

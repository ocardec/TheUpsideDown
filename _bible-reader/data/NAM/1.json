    "title": "Nahum",
    "order": 34,
    "numberOfChapters": 3,
    "sha256": "b2d91c435e1222d177f05f52314409e1f419acad928cab2127d2761428324309",
    "firstChapterApiLink": "/api/BSB/NAM/1.json",
    "lastChapterApiLink": "/api/BSB/NAM/3.json",

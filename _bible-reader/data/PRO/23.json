    "title": "Proverbs",
    "order": 20,
    "numberOfChapters": 31,
    "sha256": "65b0193f19bcb5d467eebbb09e2a9e8d1653a03fbe5c1c6098dd758f3c819162",
    "firstChapterApiLink": "/api/BSB/PRO/1.json",

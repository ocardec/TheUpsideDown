  "chapter": {
    "number": 134,
    "content": [
      {
        "type": "heading",
        "content": [
          "Bless the LORD, All You Servants"
        ]
      },
      {
        "type": "hebrew_subtitle",
        "content": [
          "A song of ascents."
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Come, bless the LORD,",
            "poem": 1
          },
          {
            "text": "all you servants of the LORD",
            "poem": 2
          },
          {
            "text": "who serve by night",
            "poem": 1
          },
          {
            "text": "in the house of the LORD!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Lift up your hands to the sanctuary",
            "poem": 1
          },
          {
            "text": "and bless the LORD!",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "May the LORD, the Maker of heaven and earth,",
            "poem": 1
          },
          {
            "text": "bless you from Zion.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []

        "reference": {
          "chapter": 39,
          "verse": 23
        }
      },
      {
        "noteId": 3218,
        "caller": "+",
        "text": "Or he snorts, ‘Aha!’",
        "reference": {
          "chapter": 39,
          "verse": 25
        }
      }
    ]
  },

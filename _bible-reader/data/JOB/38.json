    "translationId": "BSB",
    "name": "Job",
    "commonName": "Job",
    "title": "Job",
    "order": 18,
    "numberOfChapters": 42,
    "sha256": "bea50752c18472b7e4fb83842056cce3a7cc4d985beb7cc6a25b6a1fcde44b11",
    "firstChapterApiLink": "/api/BSB/JOB/1.json",

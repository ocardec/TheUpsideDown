    "title": "Jeremiah",
    "order": 24,
    "numberOfChapters": 52,
    "sha256": "3433db1d3041ce1702236ecc3b937dc5436d022d7872a09907d7e5730c08855e",
    "firstChapterApiLink": "/api/BSB/JER/1.json",

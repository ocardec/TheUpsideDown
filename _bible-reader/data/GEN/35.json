        "reference": {
          "chapter": 35,
          "verse": 15
        }
      },
      {
        "noteId": 2281,
        "caller": "+",
        "text": "Ben-oni could mean son of my sorrow or son of my strength.",
        "reference": {
          "chapter": 35,
          "verse": 18
        }
      },
      {
        "noteId": 2282,
        "caller": "+",
        "text": "Benjamin means son of my right hand.",
        "reference": {
          "chapter": 35,
          "verse": 18
        }
      }
    ]
  },

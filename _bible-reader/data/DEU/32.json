    "title": "Deuteronomy",
    "order": 5,
    "numberOfChapters": 34,
    "sha256": "0e6d69387d7fa57fbeb36bb3b46ffca7d64297b4913d178614543fd030e1a49f",
    "firstChapterApiLink": "/api/BSB/DEU/1.json",
